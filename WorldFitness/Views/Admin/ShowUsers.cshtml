<style>
    table, th, td{
        border: 1px solid black;
        padding: .5em;
    }
</style>
@{
    var count = 1;
    <p>Users List:</p>
    @if(ViewBag.UsersList.Count != 0)
    {
        <table>
            <tr>
                <th> S.No</th>
                <th>Name</th>
                <th>Email</th>
                <th>Role</th>
            </tr>
            @foreach(var eachUser in (List<User>)ViewBag.UsersList)
            {
                <tr>
                    <td>@count </td>
                    <td>@eachUser.Name</td>
                    <td>@eachUser.EmailId</td>
                    <td>@eachUser.Role</td>
                </tr>
                count += 1;
            }
        </table>

    }
}
